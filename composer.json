{
    "name": "winwin/db-tools",
    "type": "library",
    "description": "Database tools for schema and data management",
    "keywords": [],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "yewenbin@winwin-inc.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^2.5",
        "jdorn/sql-formatter": "^1.2",
        "kuiper/db": "^0.6",
        "roave/better-reflection": "^4.3",
        "symfony/console": "^5.1",
        "symfony/yaml": "^5.1",
        "vlucas/phpdotenv": "^5.1"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.34",
        "phpstan/phpstan-strict-rules": "^0.12.4",
        "phpunit/phpunit": "^7.0",
        "winwin/composer-phpcs-plugin": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "winwin\\db\\tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "winwin\\db\\tools\\": "tests/"
        }
    },
    "bin": "bin/db"
}
